{
  "name": "@freshworks/freshworks-api-sdk",
  "version": "0.0.1",
  "private": true,
  "description": "Node.js SDK for Freshworks HTTP APIs",
  "engines": {
    "node": ">=10.x"
  },
  "files": [
    "/dist"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc --watch",
    "test": "jest --coverage",
    "format": "prettier -w .",
    "clean": "rm -rf coverage/ dist/ api-docs/",
    "docs:sdk": "rm -rf api-docs && jsdoc -c .jsdoc.conf.json"
  },
  "author": "Freshworks Inc",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "jsdoc": "^3.6.7",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest/presets/js-with-ts"
  }
}
